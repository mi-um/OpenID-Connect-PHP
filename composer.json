{
	"name" : "mi-um/openid-connect-php",
	"description" : "Bare-bones OpenID Connect client",
	"license" : "Apache-2.0",
	"require" : {
		"php" : ">=7.2",
		"phpseclib/phpseclib" : "~2.0",
		"paragonie/random_compat" : ">=2",
		"ext-json" : "*",
		"ext-curl" : "*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^4.8",
		"roave/security-advisories" : "dev-master"
	},
	"minimum-stability": "dev",
	"archive" : {
		"exclude" : [
			".*"
		]
	},
	"autoload" : {
		"psr-4" : {
			"mium\\oidc\\" : "src"
		}
	}
}
